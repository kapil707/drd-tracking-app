<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.DrdTrackingApp" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your dark theme here. -->
        <item name="colorPrimary">@color/header_bg_dark</item>
    </style>

    <style name="nav_bar_header_theme" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="android:background">@color/nav_bar_header_bg_dark</item>
    </style>

    <style name="nav_bar_text_theme" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="android:textColor">@color/nav_bar_text_light</item>
    </style>

    <style name="header_login_page" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="color">@color/header_bg_dark</item>
    </style>

    <style name="page_bg_theme" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="android:background">@color/page_bg_dark</item>
    </style>

    <style name="page_textclr_theme" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="android:textColor">@color/page_textclr_dark</item>
    </style>

    <style name="button_theme" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="android:textColor">@color/button_textclr_dark</item>
        <item name="android:backgroundTint">@color/button_bg_dark</item>
    </style>

    <style name="button_dis_theme" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="android:textColor">@color/button_dis_textclr_dark</item>
        <item name="android:backgroundTint">@color/button_dis_bg_dark</item>
    </style>

    <style name="textbox_theme" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="android:tint">@color/textbox_bg_dark</item>
        <item name="android:background">@color/textbox_bg_dark</item>
        <item name="android:textColor">@color/textbox_textclr_dark</item>
        <item name="android:textColorHint">@color/textbox_textclr_hint_dark</item>
    </style>

    <style name="textbox_shap_theme" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="android:tint">@color/textbox_bg_dark</item>
        <item name="android:background">@color/textbox_bg_dark</item>
        <item name="android:color">@color/textbox_bg_dark</item>
    </style>

    <style name="textbox_fafa_theme" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="android:tint">@color/textbox_fafa_icon_dark</item>
        <item name="android:background">@color/textbox_bg_dark</item>
    </style>

    <style name="textbox_border_theme" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="color">@color/header_bg_dark</item>
    </style>

    <style name="list_item_bg_theme" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="android:background">@color/list_item_bg_dark_primary</item>
    </style>

    <style name="home_page_camra_clr_theme" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="android:background">@color/home_page_camra_clr_dark</item>
    </style>

    <style name="list_item_bg_theme2" parent="Theme.Material3.DayNight.NoActionBar">
        <item name="android:background">@color/list_item_bg2_dark_primary</item>
    </style>
</resources>